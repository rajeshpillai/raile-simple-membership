<!DOCTYPE html>
<html>
  <head>
    <title>Membership</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div id="container">
      <div id="user_header">
        <%= link_to "Sign Up", new_user_path %>
      </div>
      <%= yield %>
    </div>
  </body>
</html>
